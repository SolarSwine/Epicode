{
  "/register": {
    "post": {
      "responses": {
        "200": {
          "description": "User registered successfully",
          "body": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer"
              },
              "username": {
                "type": "string"
              },
              "email": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
